  -H "Content-Type: application/json" \
  -H "Authorization: Bearer YOUR API TOKEN" \
  -d '{
    "model": "meta/llama3-70b-instruct",
    "messages": [{"role":"user","content":"Write a sentence on AI"}],
    "temperature": 0.5,   
    "top_p": 1,
    "max_tokens": 1024,
    "stream": true                
  }'


